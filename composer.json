{
    "name":"asconsulting/isotope_multialias",
    "description":"Module to allow for multiple product aliases for Isotope eCommerce and the Contao CMS.",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "isotope", "domain", "language", "alias"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": "~5.4 || ~7.0",
        "contao/core-bundle": "^3.5.2 || ~4.4",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "Asc\\": [
                "system/modules/isotope_multialias/library"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_multialias": "system/modules/isotope_multialias"
            }
        }
    },
    "minimum-stability": "dev"
}