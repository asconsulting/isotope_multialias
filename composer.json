{
    "name":"asconsulting/isotope_multialias",
    "description":"Module to allow for multiple product aliases (per site root) for Isotope eCommerce and the Contao CMS.",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "isotope", "domain", "language", "alias", "domain"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/core-bundle": "~4.13",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MultiAlias\\": [
                "system/modules/isotope_multialias/library/MultiAlias"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_multialias": "system/modules/isotope_multialias"
            }
        }
    },
    "minimum-stability": "dev"
}